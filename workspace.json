{
  "version": 1,
  "projects": {
    "terminology-api": {
      "root": "apps/terminology-api",
      "sourceRoot": "apps/terminology-api/src",
      "projectType": "application",
      "prefix": "terminology-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/terminology-api",
            "main": "apps/terminology-api/src/main.ts",
            "tsConfig": "apps/terminology-api/tsconfig.app.json",
            "assets": ["apps/terminology-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/terminology-api/src/environments/environment.ts",
                  "with": "apps/terminology-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "terminology-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/terminology-api/.eslintrc",
            "tsConfig": [
              "apps/terminology-api/tsconfig.app.json",
              "apps/terminology-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/terminology-api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/terminology-api/jest.config.js",
            "tsConfig": "apps/terminology-api/tsconfig.spec.json"
          }
        }
      }
    },
    "terminology-interfaces": {
      "root": "libs/terminology-interfaces",
      "sourceRoot": "libs/terminology-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/terminology-interfaces/tsconfig.lib.json",
              "libs/terminology-interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/terminology-interfaces/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/terminology-interfaces/jest.config.js",
            "tsConfig": "libs/terminology-interfaces/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "terminology-api"
}
